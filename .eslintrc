{
  "extends": [
    "airbnb",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript",
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".tsx", ".ts"] }]
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
